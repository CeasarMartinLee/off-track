{
  "expo": {
    "name": "Off-Track",
    "description": "This is an app for exploring urban city scenes whilst enjoying an accompanying soundtrack",
    "slug": "off-track-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "android":{
      "package":"com.offtrack.offtrack"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/app-icon.png",
    "splash": {
      "image": "./src/images/launch.png",
      "resizeMode": "contain",
      "backgroundColor": "#22262E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
